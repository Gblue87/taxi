{
    "name": "newvision/content-bundle",
    "description": "This Symfony bundle provide easy way to create Content pages",
    "keywords": ["Symfony", "Content", "Bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hristo Hristoff",
            "email": "hristo.hristov@newvision.bg"
        }
    ],
    "require": {
        "newvision/core-bundle": "dev-master",
        "newvision/seo-bundle": "dev-master",
        "newvision/publish-workflow-bundle": "dev-master"
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "git@dev.newvisiongroup.com:symfony-bundles/newvision-core-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@dev.newvisiongroup.com:symfony-bundles/newvision-seo-bundle.git"
        }
    ],
    "autoload": {
        "psr-0": { "NewVision\\ContentBundle": "" }
    },
    "target-dir": "NewVision/ContentBundle"
}