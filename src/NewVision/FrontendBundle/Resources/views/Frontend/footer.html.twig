
<footer itemscope="" itemtype="http://schema.org/WPFooter">
    <div class="footer-top">
        <div class="center">
            {% if footerBlocks is defined and footerBlocks is not null and footerBlocks|length %}
                {% for fb in footerBlocks if fb is defined and fb is not null and fb|length %}
                    <div class="fbox">
                        {{ fb.description|raw }}
                    </div>
                {% endfor %}
            {% endif %}
            <div class="fbox fbox3">
                {{footerBlock4.description|raw}}
                <div class="social">
                    <span>Follow</span>
                    {% if settings.get('fb') is defined and settings.get('fb') is not null and settings.get('fb')|length %}
                         <a href="{{ settings.get('fb') }}" target="_blank" class="fb"></a>
                    {% endif %}
                    {% if settings.get('gplus') is defined and settings.get('gplus') is not null and settings.get('gplus')|length %}
                        <a href="https:{{ settings.get('gplus') }}" target="_blank" class="gp"></a>
                    {% endif %}
                    {% if settings.get('twitter') is defined and settings.get('twitter') is not null and settings.get('twitter')|length %}
                        <a href="{{ settings.get('twitter') }}" target="_blank" class="tw"></a>
                    {% endif %}
                    {% if settings.get('linkedin') is defined and settings.get('linkedin') is not null and settings.get('linkedin')|length %}
                        <a href="{{ settings.get('linkedin') }}" class="in"></a>
                    {% endif %}
                </div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="footer-bottom">
        <div class="center">
            <div>
                Â© {{"now"|date('Y')}} {{ 'all_rights_reserved'|trans({}, 'messages')}}
                &nbsp;&nbsp;&nbsp;
                <img src="/images/pay.png" alt="pay" />
            </div>
        </div>
    </div>

</footer>

